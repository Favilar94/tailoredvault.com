  <body>
    <h1>Upload File</h1>
    <form action="/files" method="POST" enctype="multipart/form-data">
      <h3>Description</h3><input type="text" name="description" />
      <h3>username</h3><input type="text" name="username" />
      <h3>userID</h3><input type="text" name="userID" />
      <h3>pryvacyID</h3><input type="text" name="privacyID" />
      <h3>customshare</h3><input type="text" name="customShare" />
      <input type="file" name="file" multiple/>
      <button type="submit">Upload</button>
    </form>
  </body>
</html>
